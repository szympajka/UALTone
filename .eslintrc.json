{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "react/jsx-filename-extension": [0],
        "react/prefer-stateless-function": [0],
        "func-names":"off",
        "global-require":"off",
        "no-nested-ternary":"off"
    }
}